    "163306143649b6db9e": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"content product_header\">\n<div class=\"content_bg\">\n{% if product.metafields.mitosaya_fields.figure_youtube %}\n<iframe class=\"lazyload bgimg\" width=\"560\" height=\"315\" data-src=\"https:\/\/www.youtube.com\/embed\/{{ product.metafields.mitosaya_fields.figure_youtube }}\" title=\"YouTube video player\" frameborder=\"0\" allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen><\/iframe>\n\n{% elsif product.metafields.mitosaya_fields.figure_video %}\n<video preload=\"none\" data-autoplay loop playsinline muted class=\"lazyload bgimg\">\n<source type=\"video\/mp4\" src=\"{{ product.metafields.mitosaya_fields.figure_video | file_url }}\">\n<\/video>\n{% elsif product.metafields.mitosaya_fields.figure_image %}\n<img src=\"{{ product.metafields.mitosaya_fields.figure_image | file_url }}\" class=\"lazyload bgimg\">\n{% endif %}<\/div><div class=\"content_inner\">\n<div class=\"content_body -center\">\n<div class=\"product_header_labels\">\n<p class=\"product_header_labels_main\">TASTE<\/p>\n<p>ONLINE SHOP<\/p>\n{% if product.collections %}\n<ul class=\"product_header_collections\">\n{% for collection in product.collections %}\n<li>{{ collection.title }}<\/li>\n{% endfor %}\n<\/ul>\n{% endif %}\n<\/div>\n{% if product.metafields.mitosaya_fields.summary %}\n<div class=\"product_header_summary\">\n{{ product.metafields.mitosaya_fields.summary | newline_to_br }}\n<\/div>\n{% endif %}\n<div class=\"product_header_titles\">\n<h1 class=\"product_header_titles_main\">{{ product.title }}<\/h1>\n{% if product.metafields.mitosaya_fields.subtitle_jp %}\n<p class=\"product_header_titles_sub\">{{ product.metafields.mitosaya_fields.subtitle_jp }}<\/p>\n{% endif %}\n<\/div>\n{% if product.metafields.mitosaya_fields.alcohol %}\n<p class=\"product_header_alcohol\">{{ product.metafields.mitosaya_fields.alcohol }}% vol<\/p>\n{% endif %}\n{% if product.variants %}\n<ul class=\"product_header_variants\">\n{% for variant in product.variants %}\n<li>{{ variant.title }} {{ variant.price | money }}\n{% if product.available %}in stock{% else %}out of stock{% endif %}<\/li>\n{% endfor %}\n<\/ul>\n{% endif %}\n<\/div><\/div><\/div>"
      }
    },
    "163428824905094aa8": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"content\">\n\t<div class=\"content_inner -narrow\">\n\t\t{% if product.media %}\n\t\t<div class=\"product_imgs\">\n\t\t{% for media in product.media %}\n\t\t  {% case media.media_type %}\n\t\t    {% when 'image' %}\n\t\t      <div class=\"product_imgs_item\">\n\t\t        <img src=\"{{ media | img_url: '1200x1200'}}\" alt=\"{{ media.alt }}\">\n\t\t       <\/div>\n\t\t    {% when 'external_video' %}\n\t\t      <div class=\"product_imgs_item -media\">\n\t\t        {{ media | external_video_tag }}\n\t\t      <\/div>\n\t\t    {% when 'video' %}\n\t\t      <div class=\"product_imgs_item -mov\">\n\t\t        {{ media | video_tag: controls: true }}\n\t\t      <\/div>\n\t\t    {% when 'model' %}\n\t\t      <div class=\"product_imgs_item -model\">\n\t\t        {{ media | model_viewer_tag }}\n\t\t      <\/div>\n\t\t  {% endcase %}\n\t\t{% endfor %}\n\t\t<\/div>\n\t\t{% endif %}\n\t<\/div>\n<\/div>"
      }
    },
